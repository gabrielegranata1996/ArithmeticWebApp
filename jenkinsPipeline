pipeline{
    agent any

    stage('Compile Stage'){
        steps{
            withMaven(maven : 'Maven Default'){
                sh 'mvn compile'
            }
        }
    }

    stage ('Package Stage'){
        steps{
            withMaven(maven : 'Maven Default'){
                sh 'mvn clean package'
            }
        }
    }

    stage ('Install Stage'){
        steps{
            withMaven(maven : 'Maven Default'){
                sh 'mvn clean install'
            }
        }
    }
}